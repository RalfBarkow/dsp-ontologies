                    owl:onProperty dsp-repo:hasTemporalCoverage ;
                    owl:minCardinality "0"^^xsd:nonNegativeInteger ] ,
                    [ rdf:type owl:Restriction ;
                    owl:onProperty dsp-repo:hasSpatialCoverage ;
                    owl:minCardinality "0"^^xsd:nonNegativeInteger ] ,
                    [ rdf:type owl:Restriction ;
                    owl:onProperty dsp-repo:hasFunding ;
                    owl:minCardinality "0"^^xsd:nonNegativeInteger ] ,
